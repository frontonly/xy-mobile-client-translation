  "answer_records": {
    "answer_records": {
      "on_review_record": {
        "modal1": {
          "title": "提示",
          "message": "{{nickname}}的答卷已发回，无法进行批阅",
          "ok": "确定"
        },
        "modal2": {
          "title": "提示",
          "message": "{{nickname}}的答卷正在提交中，无法进行批阅，请稍后再尝试",
          "ok": "确定"
        },
        "modal3": {
          "title": "是否继续批阅或仅浏览学生答卷？",
          "preview": "浏览",
          "check": "批阅",
          "cancel": "取消"
        }
      },
      "on_preview_record": {
        "modal": {
          "title": "提示",
          "message": "{{nickname}}的答卷已发回，无法进行浏览",
          "ok": "确定"
        }
      },
      "on_share_record": {
        "modal": {
          "title": "提示",
          "message": [
            "",
            "答卷取消公开生效，除该答卷提交者外的其它学生将不可再查看该答卷",
            "答卷实名公开生效，学生可在示范答卷展示区查看该答卷",
            "答卷匿名公开生效，学生可在示范答卷展示区查看该匿名答卷"
          ],
          "ok": "确定",
          "cancel": "取消"
        }
      },
      "handle_delete": {
        "modal": {
          "title": "提示",
          "message": "是否确定删除该答题记录？删除后无法恢复记录，请谨慎操作！",
          "ok": "删除",
          "cancel": "取消"
        }
      },
      "handle_batch_mark": {
        "toast": "批量打分暂不支持，请前往web端访问！"
      },
      "render_content": {
        "empty": "暂无数据"
      },
      "render_sort": {
        "sort": {
          "0": "按名称",
          "1": "按提交时间"
        },
        "grade_type": {
          "true": "按得分",
          "false": "按评级"
        },
        "mark_mode": {
