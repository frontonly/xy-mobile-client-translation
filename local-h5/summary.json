    "mark_distribute": {
      "handle_recheck_again": {
        "modal": {
          "title": "提示",
          "message": "重新发起{{mark_mode_text}}会删除全部已有的{{mark_mode_text}}批改记录，且每天只能发起一次，是否确定重新发起？",
          "ok": "确定",
          "cancel": "取消"
        },
        "mark_mode": {
          "true": "自评",
          "false": "互评"
        }
      },
      "renderContent": {
