  "answer_records": {
    "answer_records": {
      "on_review_record": {
        "modal1": {
          "title": "Notice",
          "message": "{{nickname}}'s answer sheet has been sent back, cannot be reviewed",
          "ok": "Confirm"
        },
        "modal2": {
          "title": "Notice",
          "message": "{{nickname}}'s answer sheet is being submitted, cannot be reviewed, please try again later",
          "ok": "Confirm"
        },
        "modal3": {
          "title": "Do you want to continue reviewing or just browse the student's answer sheet?",
          "preview": "Browse",
          "check": "Review",
          "cancel": "Cancel"
        }
      },
      "on_preview_record": {
        "modal": {
          "title": "Notice",
          "message": "{{nickname}}'s answer sheet has been sent back, cannot be browsed",
          "ok": "Confirm"
        }
      },
      "on_share_record": {
        "modal": {
          "title": "Notice",
          "message": [
            "",
            "The answer sheet cancellation is effective, and other students will not be able to view this answer sheet except for the submitter",
            "The answer sheet public release is effective, students can view this answer sheet in the demonstration answer sheet display area",
            "The anonymous answer sheet public release is effective, students can view this anonymous answer sheet in the demonstration answer sheet display area"
          ],
          "ok": "Confirm",
          "cancel": "Cancel"
        }
      },
      "handle_delete": {
        "modal": {
          "title": "Notice",
          "message": "Are you sure you want to delete this answer record? Once deleted, the record cannot be recovered, please operate with caution!",
          "ok": "Delete",
          "cancel": "Cancel"
        }
      },
      "handle_batch_mark": {
        "toast": "Batch scoring is not supported, please visit the web portal!"
      },
      "render_content": {
        "empty": "No data"
      },
      "render_sort": {
        "sort": {
          "0": "By name",
          "1": "By submission time"
        },
        "grade_type": {
          "true": "By score",
          "false": "By rating"
        },
        "mark_mode": {
          "true": "Self-assessment",
          "false": "Peer review"
        },
        "check_bar": {
          "task_finished": " people have {{mark_mode_text}},",
          "task_not_finished": " people waiting for {{mark_mode_text}}",
          "checked": " reviewed,",
          "need_check_num": " waiting for review",
          "go_check": "Go review"
        },
        "sort_bar": {
          "recheck": "View {{mark_mode_text}} distribution results",
          "picker": {
            "title": "Sort"
          },
          "batch_mark": "Batch scoring"
        },
        "checkbox": "Show all",
        "search_bar": {
          "placeholder": "Enter name",
          "cancel_text": "Cancel"
        }
      },
      "render_summary_card": {
        "assign_count": "Should participate",
        "started": "Participated",
        "started_finished": "Submitted {{started_finished}}",
        "started_not_finished": "Not submitted {{started_not_finished}}",
        "not_start": "Not participated"
      },
      "render_class_filter": {
        "picker": {
          "title": "Sort"
        }
      }
    },
    "mark_distribute": {
      "handle_recheck_again": {
        "modal": {
          "title": "Notice",
          "message": "Re-launching {{mark_mode_text}} will delete all existing {{mark_mode_text}} correction records, and can only be launched once a day, are you sure you want to re-launch?",
          "ok": "Confirm",
          "cancel": "Cancel"
        },
        "mark_mode": {
          "true": "Self-assessment",
          "false": "Peer review"
        }
      },
      "renderContent": {
        "mark_mode": {
          "true": "Self-assessment",
          "false": "Peer review"
        },
        "distribute_bar": {
          "text": "Distribution result: ",
          "is_success": {
            "true": "Successful",
            "false": "Failed"
          },
          "distribute": "Redistribute {{mark_mode_text}}"
        },
        "remind_bar": {
          "remind": "*Students who have not been assigned review papers cannot participate in {{mark_mode_text}}, and using \"Redistribute {{mark_mode_text}}\" will delete the correction record and redistribute the paper."
        }
      },
      "renderHeader": {
        "title": "Distribution result"
      }
    },
    "records": {
      "record_status": "Review status: ",
