{
  "onShowMoreMessage": {
    "toast": "Current network environment is poor, please try again later"
  },
  "renderHeader": {
    "title": "Message Center"
  },
  "render": {
    "MsgSettingModal": {
      "render": {
        "title": "Message Notification Settings",
        "desc": "*After unchecking, this type of task will no longer send message notifications",
        "check": "Select All",
        "items": [
          "Task Messages",
          "Course Announcements",
          "Group Messages",
          "Discussion Post Messages",
          "Q&A Messages"
        ]
      }
    },
    "MessageGuideModal": {
      "renderMoreContent": {
        "btns": ["Skip", "Got it"]
      },
      "renderContent": {
        "title": [
          "The message center has been updated, ",
          "let's see what's changed!"
        ],
        "btns": ["Check Now", "Skip"]
      }
    }
  },

  "sub_page": {
    "common": {
      "MsgOperationModal": {
        "render": {
          "title": "List Options",
          "remind": "Only keep messages within seven days",
          "status": ["View Unprocessed Only", "View Unread Only"],
          "btns": ["Mark All as Read", "Clear All"],
          "btnDesc": "*You can mark all unread messages in the current column as read (up to 100)"
        }
      }
    },
    "MessageCoursePage": {
      "courseFilter": {
        "default": "All Courses"
      },
      "onEnterMsgDetail": {
        "title": "Course Message Details"
      },
      "onDeleteMsg": {
        "alert": {
          "title": "Prompt",
          "content": "Do you want to delete this message?",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "onEnterAppraise": {
        "title": "Evaluation Activity"
      },
      "onReadAll": {
        "toast": "No {{type}} for now",
        "alert": {
          "title": "Please Confirm",
          "content": "Are you sure you want to mark all {{type}} (up to 100) as read?",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "onDeleteAll": {
        "toast": "No {{type}} for now",
        "alert": {
          "title": "Please Confirm",
          "content": "Are you sure to delete all {{type}} (up to 100)? This action cannot be undone",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "renderHeader": {
        "title": "Course Messages"
      },
      "renderContent": {
        "MessageList": {
          "renderItem": {
            "text": "Go to Evaluation Activity",
            "btn": "Delete"
          },
          "renderListEmptyComponent": {
            "unread": "Unread",
            "title": "No {{read}}{{type}} for now"
          }
        }
      },
      "sub_page": {
        "MessageCourseDetailPage": {
          "onDeleteMsg": {
            "alert": {
              "title": "Prompt",
              "content": "Do you want to delete this message?",
              "btns": ["Cancel", "Confirm"]
            }
          },
          "onEnterAppraise": {
            "title": "Evaluation Activity"
          },
          "renderContent": {
            "btn": "Go to Evaluation Activity"
          },
          "renderHeader": {
            "btn": "Delete"
          }
        }
      }
    },
    "MessagePersonPage": {
      "onEnterMsgDetail": {
        "title": "Personal Message Details"
      },
      "onDeleteMsg": {
        "alert": {
          "title": "Prompt",
          "content": "Do you want to delete this message?",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "onReadAll": {
        "toast": "No personal messages for now",
        "alert": {
          "title": "Please Confirm",
          "content": "Are you sure you want to mark all personal messages (up to 100) as read?",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "onDeleteAll": {
        "toast": "No personal messages for now",
        "alert": {
          "title": "Please Confirm",
          "content": "Are you sure to delete all personal messages (up to 100)? This action cannot be undone",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "renderHeader": {
        "title": "Personal Messages"
      },
      "renderContent": {
        "MessageList": {
          "renderItem": {
            "btn": "Delete"
          },
          "renderListEmptyComponent": {
            "unread": "Unread",
            "title": "No {{read}} personal messages for now"
          }
        }
      },
      "sub_page": {
        "MessageCourseDetailPage": {
          "onDeleteMsg": {
            "alert": {
              "title": "Prompt",
              "content": "Do you want to delete this message?",
              "btns": ["Cancel", "Confirm"]
            }
          },
          "renderHeader": {
            "btn": "Delete"
          }
        }
      }
    },
    "MessageSystemPage": {
      "onEnterMsgDetail": {
        "title": "System Message Details"
      },
      "onDeleteMsg": {
        "alert": {
          "title": "Prompt",
          "content": "Do you want to delete this message?",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "onEnterGroup": {
        "toasts": [
          "Entered the new course after splitting",
          "Entered the new course after merging"
        ]
      },
      "onReadAll": {
        "toast": "No system messages for now",
        "alert": {
          "title": "Please Confirm",
          "content": "Are you sure you want to mark all system messages (up to 100) as read?",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "onDeleteAll": {
        "toast": "No system messages for now",
        "alert": {
          "title": "Please Confirm",
          "content": "Are you sure to delete all system messages (up to 100)? This action cannot be undone",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "renderHeader": {
        "title": "System Messages"
      },
      "renderContent": {
        "MessageList": {
          "renderItem": {
            "btn": "Delete"
          },
          "renderListEmptyComponent": {
            "unread": "Unread",
            "title": "No {{read}} system messages for now"
          }
        }
      },
      "sub_page": {
        "MessageCourseDetailPage": {
          "onDeleteMsg": {
            "alert": {
              "title": "Prompt",
              "content": "Do you want to delete this message?",
              "btns": ["Cancel", "Confirm"]
            }
          },
          "renderHeader": {
            "btn": "Delete"
          }
        }
      }
    },
    "MessageTodoPage": {
      "onEnterMsgDetail": {
        "title": "To-Do Items Message Details"
      },
      "onEnterMemberAdd": {
        "title": "Add New Member"
      },
      "onAcceptPress": {
        "title": "Select Class",
        "toast": "Mobile end does not support handling this type of message"
      },
      "onDeleteAll": {
        "toast": "No to-do items messages for now",
        "alert": {
          "title": "Please Confirm",
          "content": "Are you sure to delete all to-do items (up to 100)? This action cannot be undone",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "onDeleteMsg": {
        "alert": {
          "title": "Prompt",
          "content": "Do you want to delete this message?",
          "btns": ["Cancel", "Confirm"]
        }
      },
      "renderHeader": {
        "title": "To-Do Items"
      },
      "renderContent": {
        "MessageList": {
          "renderItem": {
            "btns": ["Agree", "Refuse", "Handle in Course"],
            "texts": ["Agreed", "Refused"]
          },
          "renderListEmptyComponent": {
            "unread": "Unprocessed",
            "title": "No {{read}} to-do items for now"
          }
        }
      },
      "sub_page": {
        "MessageCourseDetailPage": {
          "onDeleteMsg": {
            "alert": {
              "title": "Prompt",
              "content": "Do you want to delete this message?",
              "btns": ["Cancel", "Confirm"]
            }
          },
          "onAcceptPress": {
            "title": "Select Class",
            "toast": "Mobile end does not support handling this type of message"
          },
          "onEnterMemberAdd": {
            "title": "Add New Member"
          },
          "renderContent": {
            "btns": ["Agree", "Refuse", "Handle in Course"]
          },
          "renderHeader": {
            "btn": "Delete"
          }
        }
      }
    },
    "ChatC2CPage": {
      "onEnterChat": {
        "title": "Private Message - {{title}}",
        "toast": "Current network environment is poor, please try again later",
        "toast2": "The other party has not logged into the system, unable to initiate a private message"
      },
      "renderContent": {
        "placeholder": "Please enter a name",
        "MessageListItem": {
          "renderItemOperation": {
            "title": "Delete"
          },
          "renderMsg": {
            "title": "[Image Message]",
            "title2": "[Emoji Message]"
          }
        }
      },
      "render": {
        "title": "Private Message"
      },
      "sub_page": {
        "ChatC2CRoomPage": {
          "onEnterHistory": {
            "title": "Chat History"
          },
          "renderContent": {
            "ChatMessageList": {
              "renderItem": {
                "text": "Message from 【{{name}}】"
              }
            }
          }
        },
        "ChatC2CHistoryPage": {
          "getHistoryList": {
            "toast": "Failed to load chat history, please try again"
          },
          "renderContent": {
            "title": "Start Time",
            "title1": "End Time",
            "HistoryList": {
              "renderItem": {
                "text": "Message from 【{{name}}】"
              }
            }
          },
          "render": {
            "title": "Chat History"
          }
        }
      }
    },
    "ChatContactPage": {
      "onRefresh": {
        "text": "Friends"
      },
      "renderContent": {
        "placeholder": "Please enter a name"
      },
      "render": {
        "title": "Contacts"
      },
      "sub_page": {
        "ContactMemberPage": {
          "onEnterChat": {
            "title": "Private Message - {{title}}",
            "toast": "Current network environment is poor, please try again later",
            "toast2": "The other party has not logged into the system, unable to initiate a private message"
          },
          "renderContent": {
            "placeholder": "Please enter a name"
          }
        },
        "ContactFriendPage": {
          "onEnterChat": {
            "title": "Private Message - {{title}}",
            "toast": "Current network environment is poor, please try again later",
            "toast2": "The other party has not logged into the system, unable to initiate a private message"
          },
          "renderContent": {
            "placeholder": "Please enter a name"
          }
        }
      }
    }
  }
}
